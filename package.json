{
  "name": "anpip.com",
  "main": "expo-router/entry",
  "version": "2.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "workers": "npx ts-node workers/index.ts",
    "build:web": "npx expo export -p web",
    "build:pwa": "npx expo export -p web && bash scripts/inject-pwa.sh",
    "deploy": "npm run build:pwa && npx vercel --prod",
    "deploy:functions": "supabase functions deploy initialize-upload && supabase functions deploy get-chunk-upload-url && supabase functions deploy finalize-upload && supabase functions deploy process-video && supabase functions deploy ai-content-generator",
    "deploy:migrations": "supabase db push",
    "deploy:all": "npm run deploy:migrations && npm run deploy:functions && npm run deploy",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "k8s:apply": "kubectl apply -f kubernetes/ -n anpip",
    "k8s:status": "kubectl get pods -n anpip",
    "k8s:logs": "kubectl logs -f deployment/video-worker -n anpip",
    "generate:icons": "node generate-pwa-icons.js"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.83.0",
    "dotenv": "^17.2.3",
    "expo": "^54.0.0",
    "expo-audio": "~1.0.15",
    "expo-av": "^16.0.7",
    "expo-blur": "^15.0.7",
    "expo-camera": "~17.0.9",
    "expo-clipboard": "~8.0.7",
    "expo-file-system": "^19.0.19",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "~8.0.9",
    "expo-media-library": "~18.2.0",
    "expo-notifications": "~0.32.13",
    "expo-router": "~6.0.15",
    "expo-speech": "^14.0.7",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "expo-video": "^3.0.14",
    "expo-video-thumbnails": "~10.0.7",
    "expo-web-browser": "~15.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/ngrok": "^4.1.3",
    "@types/node": "^24.10.1",
    "@types/react": "~19.1.10",
    "expo-module-scripts": "^5.0.7",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.13",
    "react-test-renderer": "19.1.0",
